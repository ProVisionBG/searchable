{
  "name": "provision/searchable",
  "type": "library",
  "description": "Searchable indexing with MySQL full text for Laravel",
  "keywords": [
    "searchable",
    "fulltext",
    "mysql",
    "laravel"
  ],
  "homepage": "https://github.com/provisionbg/searchable",
  "license": "MIT",
  "authors": [
    {
      "name": "Venelin Iliev",
      "email": "venelin@provision.bg",
      "homepage": "https://veneliniliev.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "illuminate/database": "5.7.*|5.8.*|^6.0|^7.0|^8.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "~6.0|^7.0|^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "ProVision\\Searchable\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ProVision\\Searchable\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "ProVision\\Searchable\\ServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  }
}
